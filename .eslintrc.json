{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": false,
        "serviceworker": false
    },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ],
        "indent": [
            0
        ],
        "linebreak-style": [
            "off",
            "eol-last"
        ],
        "quotes": [
            0
        ],
        "semi": [
            0
        ]
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "FormData": true,
        "FileReader": true,
        "Blob": true,
        "navigator": true,
        "speechSynthesis": true
    }
}